        },
        {
            "name": "Dynamic ViewModels",
            "tagFrom": "<!-- Start ViewModels -->",
            "tagTo": "   <!-- End ViewModels -->",
            "template": "{{#each modules}}{{#notEqual name \"Home\"}}\t\t<Compile Include=\"$(MSBuildThisFileDirectory)ViewModels\\ResourcesViewModel\\\\{{capitalize name}}ViewModel.cs\"/>\n{{/notEqual}}{{/each}}"
        },
        {
            "name": "Dynamic Pages",
            "tagFrom": "<!-- Start Dynamic Pages -->",
